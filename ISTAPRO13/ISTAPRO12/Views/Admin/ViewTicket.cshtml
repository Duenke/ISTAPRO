@model ISTAPRO12.Models.Ticket

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>New Tickets View</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <div>
        <table class="table table-striped table-bordered">
            <tr><td>@Model.TicketNum</td></tr>
            <tr><td>@Model.SubmitDate</td></tr>
            <tr><td>@Model.OpenDate</td></tr>
            <tr><td>@Model.CloseDate</td></tr>
            <tr><td>@Model.EmpID</td></tr>
            <tr><td>@Model.CustID</td></tr>
            <tr><td>@Model.CustName</td></tr>
            <tr><td>@Model.CustEmail</td></tr>
            <tr><td>@Model.Platform</td></tr>
            <tr><td>@Model.Summary</td></tr>
            <tr><td>@Model.Issue</td></tr>
            <tr><td>@Model.Resolution</td></tr>
        </table>
        <a asp-action="NewTickets" class="btn btn-outline-primary">Back</a>
        <a asp-controller="Home" asp-action="Index" class="btn btn-outline-secondary">Home</a>
    </div>
</body>
</html>